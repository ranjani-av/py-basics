class Fibonacci:
    def __init__(self,n):
        self.n=n
    def __iter__(self):
        self.a=0
        self.b=1
        return self
    def __next__(self):
        if self.a > self.n:
            raise StopIteration
        else:
            self.a, self.b = self.b, self.a+self.b
            return self.a

n=int(input("Enter n:"))
fibo=Fibonacci(n)

for i in fibo:
    print(i)
